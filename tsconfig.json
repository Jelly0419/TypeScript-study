{
  "display": "Node 16",
  "compilerOptions": {
    "lib": ["es2021"],
    "module": "commonjs",
    "target": "es2021",
    "outDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "exclude": ["node_modules", "typeScriptStudy"],
  "files": ["src/index.ts"]
}
